{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Berry Dash Ultimate",
  "version": "1.3.0",
  "identifier": "xyz.lncvrt.bonoise-bdu",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Berry Dash Ultimate",
        "minWidth": 854,
        "minHeight": 480,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
